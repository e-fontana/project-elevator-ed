#! /usr/local/bin/vvp
:ivl_version "13.0 (devel)" "(s20221226-533-g676b36e45)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
S_0x5805b9d15920 .scope module, "sos_handler_tb" "sos_handler_tb" 2 3;
 .timescale 0 0;
v0x5805b9d3df30_0 .var "clk", 0 0;
v0x5805b9d3e000_0 .var "sos_button", 0 0;
v0x5805b9d3e0d0_0 .net "sos_mode", 0 0, v0x5805b9d3da20_0;  1 drivers
S_0x5805b9d29640 .scope module, "LOCATION" "location" 2 8, 3 1 0, S_0x5805b9d15920;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "sos_button";
    .port_info 2 /OUTPUT 1 "sos_mode";
v0x5805b9d298b0_0 .net "clk", 0 0, v0x5805b9d3df30_0;  1 drivers
v0x5805b9d3d960_0 .net "sos_button", 0 0, v0x5805b9d3e000_0;  1 drivers
v0x5805b9d3da20_0 .var "sos_mode", 0 0;
E_0x5805b9cf03d0/0 .event anyedge, v0x5805b9d3d960_0;
E_0x5805b9cf03d0/1 .event posedge, v0x5805b9d298b0_0;
E_0x5805b9cf03d0 .event/or E_0x5805b9cf03d0/0, E_0x5805b9cf03d0/1;
S_0x5805b9d3db40 .scope task, "change_clk" "change_clk" 2 31, 2 31 0, S_0x5805b9d15920;
 .timescale 0 0;
TD_sos_handler_tb.change_clk ;
    %delay 1, 0;
    %load/vec4 v0x5805b9d3df30_0;
    %inv;
    %store/vec4 v0x5805b9d3df30_0, 0, 1;
    %end;
S_0x5805b9d3dd20 .scope task, "display" "display" 2 27, 2 27 0, S_0x5805b9d15920;
 .timescale 0 0;
TD_sos_handler_tb.display ;
    %vpi_call 2 28 "$display", "sos_mode: %b", v0x5805b9d3e0d0_0 {0 0 0};
    %end;
    .scope S_0x5805b9d29640;
T_2 ;
    %wait E_0x5805b9cf03d0;
    %load/vec4 v0x5805b9d3d960_0;
    %pad/u 32;
    %pushi/vec4 1, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x5805b9d3d960_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x5805b9d3da20_0;
    %pad/u 32;
    %pushi/vec4 1, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %or;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5805b9d3da20_0, 0;
    %jmp T_2.1;
T_2.0 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5805b9d3da20_0, 0;
T_2.1 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5805b9d15920;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5805b9d3e000_0, 0, 1;
    %fork TD_sos_handler_tb.change_clk, S_0x5805b9d3db40;
    %join;
    %vpi_call 2 13 "$display", "sos_button initial: %b", v0x5805b9d3e000_0 {0 0 0};
    %vpi_call 2 14 "$display", "sos_mode initial: %b", v0x5805b9d3e0d0_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5805b9d3e000_0, 0, 1;
    %fork TD_sos_handler_tb.change_clk, S_0x5805b9d3db40;
    %join;
    %vpi_call 2 18 "$display", "sos_button final: %b", v0x5805b9d3e000_0 {0 0 0};
    %vpi_call 2 19 "$display", "sos_mode final: %b", v0x5805b9d3e0d0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5805b9d3e000_0, 0, 1;
    %fork TD_sos_handler_tb.change_clk, S_0x5805b9d3db40;
    %join;
    %vpi_call 2 23 "$display", "sos_button final: %b", v0x5805b9d3e000_0 {0 0 0};
    %vpi_call 2 24 "$display", "sos_mode final: %b", v0x5805b9d3e0d0_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
